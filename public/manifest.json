{
    "name": "BookMat",
    "version": "1.0",
    "description": "Material bookmarks based on tags!",
    "manifest_version": 2,
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "img/favicon/16x16.png",
            "32": "img/favicon/32x32.png",
            "48": "img/favicon/48x48.png",
            "128": "img/favicon/128x128.png"
        }
    },
    "icons": {
        "16": "img/favicon/16x16.png",
        "32": "img/favicon/32x32.png",
        "48": "img/favicon/48x48.png",
        "128": "img/favicon/128x128.png"
    }
}